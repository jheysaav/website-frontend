{
  "name": "personal-website-frontend",
  "private": true,
  "license": "MIT",
  "version": "1.0.0-beta",
  "engines": {
    "node": "14.x || 16.x",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "format": "prettier --ignore-path .gitignore --write *.{js,ts,json,html,md} {*/**,*/**/*}.{vue,ts,json}",
    "format:check": "prettier --ignore-path .gitignore --check *.{js,ts,json,html,md} {*/**,*/**/*}.{vue,ts,json}",
    "lint": "eslint --ext js,ts,vue --ignore-path .gitignore --fix src",
    "lint:check": "eslint --ext js,ts,vue --ignore-path .gitignore src"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@vueuse/head": "^0.7.6",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/prompt-cli": "^17.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "cssnano-preset-advanced": "^5.3.8",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.16.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.1"
  }
}
